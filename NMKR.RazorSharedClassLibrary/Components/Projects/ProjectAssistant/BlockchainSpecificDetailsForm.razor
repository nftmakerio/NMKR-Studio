@using NMKR.Shared.Classes.Projects
<MudTabs Outlined="true">
    @if (Value.cardano)
    {
        <MudTabPanel Text="Cardano" Icon="@ada">
            <EditCardanoDetailsForm IsAdmin="@IsAdmin" CustomerId="@CustomerId" @bind-Value="Value"></EditCardanoDetailsForm>
        </MudTabPanel>
    }
    @if (Value.solana)
