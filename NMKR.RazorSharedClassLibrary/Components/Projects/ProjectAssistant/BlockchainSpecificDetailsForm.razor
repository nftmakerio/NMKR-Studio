        <MudTabPanel Text="Aptos" Icon="@apt">
            <EditAptosDetailsForm IsAdmin="@IsAdmin" CustomerId="@CustomerId" @bind-Value="Value"></EditAptosDetailsForm>
    </MudTabPanel>
    }
