    {
        <MudTabPanel Text="Bitcoin" Icon="@btc">
            <EditBitcoinDetailsForm IsAdmin="@IsAdmin" CustomerId="@CustomerId" @bind-Value="Value"></EditBitcoinDetailsForm>
        </MudTabPanel>
    }
</MudTabs>


