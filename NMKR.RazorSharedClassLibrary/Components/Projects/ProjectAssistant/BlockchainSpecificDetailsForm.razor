        <MudTabPanel Text="Solana" Icon="@sol">
            <EditSolanaDetailsForm IsAdmin="@IsAdmin" CustomerId="@CustomerId" @bind-Value="Value"></EditSolanaDetailsForm>
        </MudTabPanel>
    }
