<br/>
@if (!Project.Pricelists.Any())
{
    <MudText Style="vertical-align:middle" Typo="Typo.inherit" Color="Color.Error" Class="mb-4">
        <MudIcon Style="vertical-align:middle" Icon="@Icons.Material.Filled.Error" Size="Size.Small" Color="Color.Error" />
        No pricelist found
    </MudText>
    <br />
}
else
{
    @if (Project.Enabledcoins.Contains(Coin.ADA.ToString()) == true)
    {
        if (Project.Pricelists.FirstOrDefault(x => x.Currency != Coin.SOL.ToString()) == null)
        {
            <MudText Style="vertical-align:middle" Typo="Typo.inherit" Color="Color.Error" Class="mb-4">
                <MudIcon Style="vertical-align:middle" Icon="@Icons.Material.Filled.Error" Size="Size.Small" Color="Color.Error" />
                Cardano pricelist NOT found
            </MudText>
            <br />
        }
        else
        {
            <MudText Style="vertical-align:middle" Typo="Typo.inherit" Color="Color.Success" Class="mb-4">
                <MudIcon Style="vertical-align:middle" Icon="@Icons.Material.Filled.ThumbUp" Size="Size.Small" Color="Color.Success" />
                Cardano pricelist found
            </MudText>
            <br />
        }
    }
    @if (Project.Enabledcoins.Contains(Coin.SOL.ToString()) == true)
    {
        if (Project.Pricelists.FirstOrDefault(x => x.Currency != Coin.ADA.ToString()) == null)
        {
            <MudText Style="vertical-align:middle" Typo="Typo.inherit" Color="Color.Error" Class="mb-4">
                <MudIcon Style="vertical-align:middle" Icon="@Icons.Material.Filled.Error" Size="Size.Small" Color="Color.Error" />
                Solana pricelist NOT found
            </MudText>
            <br />
        }
        else
        {
            <MudText Style="vertical-align:middle" Typo="Typo.inherit" Color="Color.Success" Class="mb-4">
                <MudIcon Style="vertical-align:middle" Icon="@Icons.Material.Filled.ThumbUp" Size="Size.Small" Color="Color.Success" />
                Solana pricelist found
            </MudText>
            <br />
        }
    }
}
@if (Project.Enabledcoins.Contains(Coin.ADA.ToString()))
