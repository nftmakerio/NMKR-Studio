@if (Project.Enabledcoins.Contains(Coin.SOL.ToString()))
{
    @if (Project.Solanacollectioncreated!=null)
    {
        <MudText Style="vertical-align:middle" Typo="Typo.inherit" Color="Color.Success" Class="mb-4">
            <MudIcon Style="vertical-align:middle" Icon="@Icons.Material.Filled.ThumbUp" Size="Size.Small" Color="Color.Success"/>
            Solana Verified Collection created
        </MudText>
        <br/>
        <MudText Typo="Typo.inherit">Solana Collection NFT: </MudText> <ShowAddress Address="@Project.Solanacollectiontransaction" Blockchain="Blockchain.Solana"></ShowAddress>
        <br/>
    }
    else
    {
        @if (Project.Solanacollectiontransaction == "<PENDING>")
        {
            <MudText Style="vertical-align:middle" Typo="Typo.inherit" Color="Color.Warning" Class="mb-4">
